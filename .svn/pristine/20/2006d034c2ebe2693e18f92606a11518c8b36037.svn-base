.rightfloat {
	float: right;
}
.topTitle {
	height: 36px;
	line-height: 36px;
	background-color: #40c199;
}
.topTitle span.text {
	color: #fff;
	margin-left: 16px;
}
.dir form {
	width: 1000px;
    margin-left: auto;
    margin-right: auto;
}
.dir form .formTitle {
	margin-top: 20px;
	margin-bottom: 10px;
}
.dir form .redstar:after {
	content: "*";
	color: red;
}
.addressbox {}
.showPerson {
	float: left;
	width: 90%;
	font-size: 14px;
	padding: 0.7% 0 0.7% 1% ;
	border: 1px solid #bfcbd9;
	border-radius: 4px;
	overflow: hidden;
}
.showPerson:focus {
	outline: none;
	border: 1px solid #4db3ff;
}
.createBtn {
	clear: both;
	padding-top: 32px;
	text-align: center;
}

.searchbox {
	color: #fff;
	margin-right: 16px;
}
.searchbox .el-input {
	float: right;
	width: 180px;
}
